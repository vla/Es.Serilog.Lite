{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Serilog": {
    "LogMinLevel": "Verbose",
    "StdOut": true,
    "Async": true,
    "FormatJson": true,
    "RollingFile": true,
    "PathFormat": "logs/log.log",
    "SourceContextFilterOptions": {
      "MinLevel": "Debug",
      "Rules": [
        {
          "LogLevel": "Warning",
          "SourceContextName": "Microsoft"
        }
      ]
    }
  }
}